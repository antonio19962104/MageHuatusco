<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <referenceContainer name="content">
        <block class="Huatusco\Men\Block\Index" name="men_index_index" template="Huatusco_Men::index.phtml" />
    </referenceContainer>


    <referenceContainer name="page.wrapper" htmlId="wrapper" >
        <block class="Huatusco\Men\Block\Index" name="men_index_index" template="Huatusco_Men::index.phtml" after="-" />
        <container htmlTag="footer" htmlClass="footer" after="-">
            <block class="Huatusco\Men\Block\Index" name="men_index_index" template="Huatusco_Men::index.phtml" after="-" />
        </container>
    </referenceContainer>

</page>
